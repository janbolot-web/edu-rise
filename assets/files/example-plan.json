{"\ufeff\n\u0422\u0435\u043c\u0430: \u042d\u043c\u0433\u0435\u043a \u044b\u0440\u043b\u0430\u0440\u044b\n\u041f\u0440\u0435\u0434\u043c\u0435\u0442: \u041a\u044b\u0440\u0433\u044b\u0437 \u0442\u0438\u043b\u0438\n\u041a\u043b\u0430\u0441\u0441\u044b: 6-\u043a\u043b\u0430\u0441\u0441\n\n\u0421\u0430\u0431\u0430\u043a\u0442\u044b\u043d \u043c\u0430\u043a\u0441\u0430\u0442\u0442\u0430\u0440\u044b\n\u0411\u0438\u043b\u0438\u043c \u0431\u0435\u0440\u04af\u04af\u0447\u04af\u043b\u04af\u043a:\n    \u2022 \u042d\u043b\u0434\u0438\u043a \u043e\u043e\u0437\u0435\u043a\u0438 \u0447\u044b\u0433\u0430\u0440\u043c\u0430\u0447\u044b\u043b\u044b\u043a\u0442\u044b\u043d \u0442\u04af\u0440\u043b\u04e9\u0440\u04af\u043d, \u04e9\u0437\u0433\u04e9\u0447\u04e9\u043b\u04af\u043a\u0442\u04e9\u0440\u04af\u043d \u0431\u0438\u043b\u04af\u04af": null, "\n    \u2022 \u042d\u043c\u0433\u0435\u043a \u044b\u0440\u043b\u0430\u0440\u044b\u043d\u044b\u043d \u043c\u0430\u0437\u043c\u0443\u043d\u0443\u043d \u0436\u0430\u043d\u0430 \u0444\u043e\u0440\u043c\u0430\u0441\u044b\u043d \u0442\u04af\u0448\u04af\u043d\u04af\u04af.\n\u04e8\u043d\u04af\u043a\u0442\u04af\u0440\u04af\u04af\u0447\u04af\u043b\u04af\u043a:\n    \u2022 \u0422\u0435\u043a\u0441\u0442\u0442\u0438 \u0442\u0430\u043b\u0434\u043e\u043e, \u043e\u0439 \u0436\u04af\u0433\u04af\u0440\u0442\u04af\u04af\u043d\u04af \u0436\u0430\u043d\u0430 \u0447\u044b\u0433\u0430\u0440\u043c\u0430\u0447\u044b\u043b\u044b\u043a\u0442\u044b \u04e9\u043d\u04af\u043a\u0442\u04af\u0440\u04af\u04af": null, "\n    \u2022 \u042b\u0440\u043b\u0430\u0440\u0434\u044b\u043d \u0440\u0438\u0442\u043c\u0438\u043d\u0435 \u0436\u0430\u043d\u0430 \u043c\u0430\u0430\u043d\u0438\u0441\u0438\u043d\u0435 \u043a\u04e9\u04a3\u04af\u043b \u0431\u0443\u0440\u0443\u0443.\n\u0422\u0430\u0440\u0431\u0438\u044f\u043b\u044b\u043a:\n    \u2022 \u042d\u043c\u0433\u0435\u043a\u043a\u0435 \u0443\u0440\u043c\u0430\u0442, \u0441\u044b\u0439\u043b\u043e\u043e \u0441\u0435\u0437\u0438\u043c\u0438\u043d \u043a\u0430\u043b\u044b\u043f\u0442\u0430\u043d\u0434\u044b\u0440\u0443\u0443": null, "\n    \u2022 \u042d\u043b\u0434\u0438\u043a \u043c\u0443\u0440\u0430\u0441\u0442\u0430\u0440\u0434\u044b \u0441\u0430\u043a\u0442\u043e\u043e \u0436\u0430\u043d\u0430 \u0443\u0440\u043c\u0430\u0442\u0442\u043e\u043e.\n\n\u041a\u04af\u0442\u04af\u043b\u04af\u04af\u0447\u04af \u043d\u0430\u0442\u044b\u0439\u0436\u0430\u043b\u0430\u0440\n    \u2022 \u041e\u043a\u0443\u0443\u0447\u0443 \u044d\u043c\u0433\u0435\u043a \u044b\u0440\u043b\u0430\u0440\u044b\u043d\u044b\u043d \u043c\u0438\u0441\u0430\u043b\u0434\u0430\u0440\u044b\u043d \u0430\u0442\u0430\u0439 \u0430\u043b\u0430\u0442": null, "\n    \u2022 \u042b\u0440\u043b\u0430\u0440\u0434\u044b\u043d \u043c\u0430\u0430\u043d\u0438\u0441\u0438\u043d \u0442\u04af\u0448\u04af\u043d\u0434\u04af\u0440\u04af\u043f, \u0442\u0430\u043b\u0434\u0430\u0439 \u0430\u043b\u0430\u0442": null, "\n    \u2022 \u042b\u0440\u043b\u0430\u0440\u0434\u044b \u044b\u0440\u0434\u0430\u043f \u0436\u0435 \u043e\u043e\u0437\u0435\u043a\u0438 \u0430\u0439\u0442\u044b\u043f \u0431\u0435\u0440\u0435 \u0430\u043b\u0430\u0442": null, "\n    \u2022 \u042d\u043c\u0433\u0435\u043a\u043a\u0435 \u0436\u0430\u043d\u0430 \u044d\u043b\u0434\u0438\u043a \u043c\u0430\u0434\u0430\u043d\u0438\u044f\u0442\u043a\u0430 \u0443\u0440\u043c\u0430\u0442 \u0441\u0435\u0437\u0438\u043c\u0438\u043d \u043f\u0430\u0439\u0434\u0430 \u043a\u044b\u043b\u0430\u0442": null, "\n    \u2022 \u0416\u0435\u043a\u0435 \u0436\u0430\u043d\u0430 \u0442\u043e\u043f\u0442\u0443\u043a \u0438\u0448-\u0430\u0440\u0430\u043a\u0435\u0442\u0442\u0435\u0440\u0434\u0435 \u0430\u043a\u0442\u0438\u0432\u0434\u04af\u04af \u043a\u0430\u0442\u044b\u0448\u0430\u0442.\n\n\u0421\u0430\u0431\u0430\u043a\u0442\u044b\u043d \u0436\u04af\u0440\u04af\u0448\u04af (5\u0415 \u043c\u043e\u0434\u0435\u043b\u0438)\n\u042d\u0442\u0430\u043f (5\u0415)\n\u041c\u0443\u0433\u0430\u043b\u0438\u043c\u0434\u0438\u043d \u0438\u0448-\u0430\u0440\u0430\u043a\u0435\u0442\u0438\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440\u0434\u044b\u043d \u0438\u0448-\u0430\u0440\u0430\u043a\u0435\u0442\u0438\n\u041a\u04af\u0442\u04af\u043b\u04af\u04af\u0447\u04af \u043d\u0430\u0442\u044b\u0439\u0436\u0430\nEngage \u2013 \u043a\u044b\u0437\u044b\u043a\u0442\u044b\u0440\u0443\u0443 (5 \u043c\u0438\u043d)\n\u00ab\u042d\u043c\u0433\u0435\u043a \u0442\u0443\u0443\u0440\u0430\u043b\u0443\u0443 \u044b\u0440\u043b\u0430\u0440\u0434\u044b \u0431\u0438\u043b\u0435\u0441\u0438\u04a3\u0435\u0440\u0431\u0438?\u00bb \u0434\u0435\u043f \u0441\u0443\u0440\u043e\u043e \u0431\u0435\u0440\u0438\u043f, \u043e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440\u0434\u044b\u043d \u043a\u044b\u0437\u044b\u0433\u0443\u0443\u0441\u0443\u043d \u043e\u0439\u0433\u043e\u0442\u043e\u0442.\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440 \u044b\u0440\u043b\u0430\u0440\u0434\u044b, \u0442\u0430\u0431\u044b\u0448\u043c\u0430\u043a\u0442\u0430\u0440\u0434\u044b, \u043c\u0430\u043a\u0430\u043b\u0434\u0430\u0440\u0434\u044b \u044d\u0441\u0442\u0435\u0448\u0435\u0442.\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440 \u0442\u0435\u043c\u0430\u0433\u0430 \u043a\u044b\u0437\u044b\u0433\u044b\u0448\u0430\u0442, \u043c\u0443\u0440\u0443\u043d\u043a\u0443 \u0431\u0438\u043b\u0438\u043c\u0434\u0438 \u044d\u0441\u043a\u0435 \u0441\u0430\u043b\u0430\u0442.\nExplore \u2013 \u0438\u0437\u0438\u043b\u0434\u04e9\u04e9 (10 \u043c\u0438\u043d)\n\u00ab\u041e\u043f \u043c\u0430\u0439\u0434\u0430\u00bb, \u00ab\u0411\u0435\u043a\u0431\u0435\u043a\u0435\u0439\u00bb, \u00ab\u0428\u044b\u0440\u044b\u043b\u0434\u0430\u045e\u00bb \u044b\u0440\u043b\u0430\u0440\u044b\u043d \u043e\u043a\u0443\u043f \u0431\u0435\u0440\u04af\u04af \u0436\u0435 \u0443\u0433\u0443\u0437\u0443\u0443": null, " \u043c\u0430\u0430\u043d\u0438\u0441\u0438\u043d \u0442\u0430\u043b\u0434\u043e\u043e.\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440 \u044b\u0440\u043b\u0430\u0440\u0434\u044b \u0443\u0433\u0443\u043f, \u043a\u043e\u0448\u043e \u044b\u0440\u0434\u0430\u0448\u0430\u0442": null, " \u043c\u0430\u0430\u043d\u0438\u0441\u0438\u043d \u0442\u0430\u043b\u0434\u0430\u0439\u0442.\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440 \u044b\u0440\u043b\u0430\u0440\u0434\u044b\u043d \u0440\u0438\u0442\u043c\u0438\u043d \u0436\u0430\u043d\u0430 \u043c\u0430\u0437\u043c\u0443\u043d\u0443\u043d \u0442\u0430\u0430\u043d\u044b\u0439\u0442.\nExplain \u2013 \u0442\u04af\u0448\u04af\u043d\u0434\u04af\u0440\u04af\u04af (15 \u043c\u0438\u043d)\n\u042d\u043c\u0433\u0435\u043a \u044b\u0440\u043b\u0430\u0440\u044b\u043d\u044b\u043d \u043c\u0430\u043a\u0441\u0430\u0442\u044b, \u0444\u043e\u0440\u043c\u0430\u0441\u044b, \u0440\u0438\u0442\u043c\u0438 \u0436\u0430\u043d\u0430 \u044d\u043b\u0434\u0438\u043d \u0442\u0443\u0440\u043c\u0443\u0448\u0443\u043d\u0434\u0430\u0433\u044b \u043e\u0440\u0434\u0443 \u0436\u04e9\u043d\u04af\u043d\u0434\u04e9 \u0442\u04af\u0448\u04af\u043d\u0434\u04af\u0440\u04e9\u0442.\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440 \u0436\u0430\u0437\u044b\u043f, \u0441\u0443\u0440\u043e\u043e\u043b\u043e\u0440\u0433\u043e \u0436\u043e\u043e\u043f \u0431\u0435\u0440\u0435\u0442": null, " \u04e9\u0437 \u0441\u04e9\u0437\u0434\u04e9\u0440\u04af \u043c\u0435\u043d\u0435\u043d \u0442\u04af\u0448\u04af\u043d\u0434\u04af\u0440\u04e9\u0442.\n\u042d\u043c\u0433\u0435\u043a \u044b\u0440\u043b\u0430\u0440\u044b\u043d\u044b\u043d \u04e9\u0437\u0433\u04e9\u0447\u04e9\u043b\u04af\u0433\u04af\u043d \u0436\u0430\u043d\u0430 \u043c\u0430\u0430\u043d\u0438\u0441\u0438\u043d \u0442\u04af\u0448\u04af\u043d\u04e9\u0442.\nElaborate \u2013 \u043a\u0435\u04a3\u0435\u0439\u0442\u04af\u04af (10 \u043c\u0438\u043d)\n\u042b\u0440\u043b\u0430\u0440\u0434\u044b \u043a\u043e\u043b\u0434\u043e\u043d\u0443\u043f \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0430\u043b\u044b\u043a \u0442\u0430\u043f\u0448\u044b\u0440\u043c\u0430\u043b\u0430\u0440\u0434\u044b \u0431\u0435\u0440\u0435\u0442: \u0442\u0435\u043a\u0441\u0442\u0442\u0438 \u0442\u0430\u043b\u0434\u043e\u043e, \u0442\u043e\u043f\u043e\u043d\u0434\u043e\u0440 \u0436\u0430\u043d\u0430 \u043a\u043e\u043b \u043a\u0443\u0440\u0430\u043b\u0434\u0430\u0440\u044b \u0442\u0443\u0443\u0440\u0430\u043b\u0443\u0443 \u0442\u0430\u043b\u043a\u0443\u0443.\n\u0422\u043e\u043f\u0442\u0443\u043a \u0436\u0430\u043d\u0430 \u0436\u0435\u043a\u0435 \u0438\u0448-\u0430\u0440\u0430\u043a\u0435\u0442\u0442\u0435\u0440\u0434\u0438 \u0430\u0442\u043a\u0430\u0440\u044b\u0448\u0430\u0442": null, " \u044b\u0440\u043b\u0430\u0440\u0434\u044b \u0436\u0430\u0442\u0442\u0430\u043f, \u043a\u043e\u0448\u043e \u044b\u0440\u0434\u0430\u0448\u0430\u0442.\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440 \u0442\u0435\u043a\u0441\u0442\u0442\u0438 \u0442\u0430\u043b\u0434\u0430\u043f, \u044b\u0440\u043b\u0430\u0440\u0434\u044b \u0430\u043a\u0442\u0438\u0432\u0434\u04af\u04af \u0430\u0442\u043a\u0430\u0440\u0430 \u0430\u043b\u044b\u0448\u0430\u0442.\nEvaluate \u2013 \u0431\u0430\u0430\u043b\u043e\u043e (5 \u043c\u0438\u043d)\n\u0421\u0443\u0440\u043e\u043e\u043b\u043e\u0440\u0434\u0443 \u0431\u0435\u0440\u0438\u043f, \u043e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440\u0434\u044b\u043d \u0431\u0438\u043b\u0438\u043c\u0438\u043d \u0442\u0435\u043a\u0448\u0435\u0440\u0435\u0442: \u00ab\u042d\u043c\u0433\u0435\u043a \u044b\u0440\u043b\u0430\u0440\u044b\u043d\u044b\u043d \u043c\u0430\u0430\u043d\u0438\u0441\u0438 \u044d\u043c\u043d\u0435\u0434\u0435?\u00bb\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440 \u0436\u043e\u043e\u043f \u0431\u0435\u0440\u0438\u043f, \u0442\u0430\u043b\u0434\u0430\u0439\u0442": null, " \u043a\u043b\u0430\u0441\u0441\u0442\u0430 \u0442\u0430\u043b\u043a\u0443\u0443 \u0436\u04af\u0440\u0433\u04af\u0437\u04e9\u0442.\n\u041e\u043a\u0443\u0443\u0447\u0443\u043b\u0430\u0440 \u0431\u0438\u043b\u0438\u043c\u0438\u043d \u0441\u0438\u0441\u0442\u0435\u043c\u0430\u043b\u0430\u0448\u0442\u044b\u0440\u044b\u043f, \u044d\u043c\u0433\u0435\u043a\u043a\u0435 \u0443\u0440\u043c\u0430\u0442 \u0441\u0435\u0437\u0438\u043c\u0438\u043d \u043a\u0430\u043b\u044b\u043f\u0442\u0430\u043d\u0434\u044b\u0440\u0430\u0442.\n\n\u04ae\u0439 \u0442\u0430\u043f\u0448\u044b\u0440\u043c\u0430\n    1. \u04ae\u0439\u0434\u04e9 \u00ab\u041e\u043f \u043c\u0430\u0439\u0434\u0430\u00bb \u0436\u0435 \u0431\u0430\u0448\u043a\u0430 \u044d\u043c\u0433\u0435\u043a \u044b\u0440\u043b\u0430\u0440\u044b\u043d \u0436\u0430\u0442\u0442\u0430\u043f \u043a\u0435\u043b\u04af\u04af": null, "\n    2. \u042d\u043c\u0433\u0435\u043a \u044b\u0440\u043b\u0430\u0440\u044b\u043d\u044b\u043d \u043c\u0430\u0430\u043d\u0438\u0441\u0438\u043d \u04e9\u0437 \u0441\u04e9\u0437\u0434\u04e9\u0440\u04af\u04a3\u04e9\u0440 \u043c\u0435\u043d\u0435\u043d \u0436\u0430\u0437\u044b\u043f \u043a\u0435\u043b\u04af\u04af": null, "\n    3. \u042d\u043c\u0433\u0435\u043a \u0436\u0430\u043d\u0430 \u0430\u043d\u044b\u043d \u043c\u0430\u0430\u043d\u0438\u0441\u0438 \u0436\u04e9\u043d\u04af\u043d\u0434\u04e9 \u043a\u044b\u0441\u043a\u0430 \u044d\u0441\u0441\u0435 \u0434\u0430\u044f\u0440\u0434\u043e\u043e.": null}